[ca]
default_ca = tiroFinale_CA

[DefaultCA]
database = index.txt

[req]
distinguished_name  = req_dn
default_md = sha512
default_bits = 2048

[req_dn]
countryName_default = CN
countryName_min     = 2
countryName_max     = 2
localityName_default= DO_NOT_TRUST
organizationName_default = tiroFinale

[clientauth]
extendedKeyUsage    = clientAuth
